				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}
	tell application "Finder"
		if length of (get selection) is 0 then
			set currentDir to target of front window
		else
			set sel to item 1 of (get selection)
			if class of sel is folder then
				set currentDir to sel
			else
				set currentDir to container of sel
			end if
		end if
		set unixDir to quoted form of POSIX path of (currentDir as text)
	end tell
	
	tell application "iTerm"
		make new terminal
		tell the current terminal
			activate current session
			launch session "Default Session"
			tell the last session
				write text "cd " &amp; unixDir
				write text "bundle exec 'middleman server'"
			end tell
		end tell
	end tell
end run</string>
